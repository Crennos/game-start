"character_replacements": Array[Dictionary]([]),
"id": "137",
"next_id": "140",
"notes": "",
"tags": PackedStringArray(),
"text": "But sometimes success is failing and realizing in failure that the only way to succeed in the game is not to play... and simply learn to walk away.",
"text_replacements": Array[Dictionary]([]),
"translation_key": "But sometimes success is failing and realizing in failure that the only way to succeed in the game is not to play... and simply learn to walk away.",
"type": &"dialogue"
},
