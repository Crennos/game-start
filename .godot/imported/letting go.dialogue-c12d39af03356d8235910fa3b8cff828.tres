"character_replacements": Array[Dictionary]([]),
"id": "120",
"next_id": "121",
"notes": "",
"tags": PackedStringArray(),
"text": "In this game, however, there will be no such moniker.",
"text_replacements": Array[Dictionary]([]),
"translation_key": "In this game, however, there will be no such moniker.",
"type": &"dialogue"
},
"121": {
